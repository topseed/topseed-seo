extends  ../../_part/_baseShell

block head
	title Page Two
	include ../../_part/_header

block main
	.mui-container-fluid
		p.
			2
		#myList
	
	script.
		TS.onAppReady(UIinit)
		function UIinit() {//don't forget #comps
			console.log('.')

			var MyBLX = BLX.extend({ // 
				getData: function() {
					const values2 = [
						{
						url: 'http://www.yahoo.com',
						head_line: 'Yahoo'
						},
						{
						url: 'http://mail.google.com',
						head_line: 'Gmail'
						},
						{
						url: 'https://www.cnet.com/news/',
						head_line: 'News'
						}
					]
					var values = {'array': values2}
					return values
					}
				})
			var blx = new MyBLX(MyBLX)

			var values = blx.getData()

			const tpl1 = document.getElementById('Lst1Tpl').text
			var data2 = {'array': values}
			var res = TW.bind(tpl1,data2)
			//console.log(res)
			const lst = document.querySelector('#myList')//ctx
			lst.innerHTML=res
		}//()


block footer
	include ../../_part/_footer

	script#Lst1Tpl(type='text/x-dot-template').
		{{~it.array :value:index}}
			<li><a href={{=value.url}}> {{=value.head_line}}</a></li>
		{{~}}

